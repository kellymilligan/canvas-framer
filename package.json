{
  "name": "canvas-poster-maker",
  "version": "1.0.0",
  "description": "HTML app to generate print-resolution artwork from canvas graphics at common paper sizes (A0 - A4)",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "devDependencies": {
    "animation-frame": "^0.3.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-loader": "^4.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "gsap": "^3.6.0",
    "html-webpack-plugin": "^4.5.0",
    "lodash": "^4.17.20",
    "node-sass": "^5.0.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "sylvester": "0.0.21",
    "three": "^0.137.0",
    "webpack": "^5.10.3",
    "webpack-dev-server": "^3.11.0",
    "webpack-zepto": "0.0.1",
    "zepto": "^1.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "jshintConfig": {
    "esversion": 6
  }
}
