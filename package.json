{
  "name": "poster-canvas",
  "version": "1.0.0",
  "description": "HTML app to generate print-resolution artwork from canvas graphics at common paper sizes (A0 - A4)",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "devDependencies": {
    "animation-frame": "^0.3.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gsap": "^1.19.0",
    "html-webpack-plugin": "^2.17.0",
    "lodash": "^4.16.3",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "sylvester": "0.0.21",
    "three": "^0.81.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-zepto": "0.0.1",
    "zepto": "^1.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0"
  },
  "jshintConfig": {
    "esversion": 6
  }
}
